@model APP_MVC_GStore.Models.TB_Usuario

@{
    ViewBag.Title = "Ingresar";
}

<h2>Ingresar</h2>

<div class="container">
    <form action="/Registro/Ingresar">
        <div class="form-group">
            <label>Email: </label>
            <input type="text" name="email" value=@ViewBag.email />
        </div>
        <div class="form-group">
            <label>Contraseña: </label>
            <input type="password" name="contra" value=@ViewBag.contra />
        </div>
        <div class="form-group">
            <input type="submit" value="Ingresar" />
        </div>

    </form>
</div>

